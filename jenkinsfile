node {

sh '''
   #!/bin/bash
   GIT_PREVIOUS_COMMIT=$(git rev-parse --short "HEAD^")
   GIT_COMMIT=$(git rev-parse --short HEAD)
   CHANGED="$(git diff --name-only $GIT_PREVIOUS_COMMIT $GIT_COMMIT ui)"
   echo "$CHANGED"
   if ($CHANGED == ' ') {
     echo "no changed"
   } else {
   echo "Changed"
   }
'''
}
